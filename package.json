  "private": true,
  "dependencies": {
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
